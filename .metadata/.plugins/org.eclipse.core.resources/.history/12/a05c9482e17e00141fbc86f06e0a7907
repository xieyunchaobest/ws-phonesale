# log4j properties #
# Apr. 21, 2007 #

# logger #
log4j.rootLogger=DEBUG,CONSOLE,ROLLING_FILE
#log4j.logger.org=DEBUG
log4j.logger.com.cattsoft=CONSOLE
log4j.additivity.com.cattsoft=true

log4j.logger.com.cattsoft.bm=DEBUG,ROLLING_FILE_BM
log4j.logger.com.cattsoft.im=DEBUG,ROLLING_FILE_IM
log4j.logger.com.cattsoft.pm=DEBUG,ROLLING_FILE_PM
log4j.logger.com.cattsoft.qr=DEBUG,ROLLING_FILE_QR
log4j.logger.com.cattsoft.sm=DEBUG,ROLLING_FILE_SM
log4j.logger.com.cattsoft.sp=DEBUG,ROLLING_FILE_SP
log4j.logger.com.cattsoft.tm=DEBUG,ROLLING_FILE_TM
log4j.logger.com.cattsoft.wm=DEBUG,ROLLING_FILE_WM
log4j.logger.com.cattsoft.sg=DEBUG,ROLLING_FILE_SG
log4j.logger.com.cattsoft.gh=DEBUG,ROLLING_FILE_GH
log4j.logger.com.cattsoft.gis=DEBUG,ROLLING_FILE_GIS


# Console Appender #
log4j.appender.CONSOLE=org.apache.log4j.ConsoleAppender
log4j.appender.CONSOLE.Target=System.out
log4j.appender.CONSOLE.layout=org.apache.log4j.PatternLayout
log4j.appender.CONSOLE.layout.ConversionPattern=%d{yyyy-MM-dd HH:mm:ss,SSS} %-5p [%C{1}.%M(%L)] - %m%n


# Rolling File #
########################
# Rolling File SPS PROJECT
########################
log4j.appender.ROLLING_FILE=org.apache.log4j.RollingFileAppender
log4j.appender.ROLLING_FILE.Threshold=DEBUG
log4j.appender.ROLLING_FILE.File=/Applications/apache-tomcat-6.0.16/webapps/log/web_mos.log
log4j.appender.ROLLING_FILE.Append=true
log4j.appender.ROLLING_FILE.MaxFileSize=10000KB
log4j.appender.ROLLING_FILE.MaxBackupIndex=100
log4j.appender.ROLLING_FILE.layout=org.apache.log4j.PatternLayout
log4j.appender.ROLLING_FILE.layout.ConversionPattern=%d{yyyy-MM-dd HH:mm:ss,SSS} %-5p [%C{1}.%M(%L)] - %m%n


########################
